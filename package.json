    "redux": "^3.5.2"
  },
  "scripts": {
    "styles": "postcss-reloader build",
    "react": "react-scripts build",
    "dev": "postcss-reloader start | react-scripts start",
    "dev-test": "node ./tasks/watch-test.js",
    "build": "rm -rf build && npm run styles && react-scripts build && cp -r build ips-web && mv ips-web build/ips-web",
    "start": "npm run build && echo http://localhost:7942 && node ./tasks/server.js",
    "lint": "node ./tasks/lint.js",
    "test": "npm run lint && jest",
    "jest": "jest"
  },
  "homepage": "http://ipstudio-master.rd.bbc.co.uk/ips-web/",
  "postcss-reloader": {
    "dir": "./src",
    "loaders": [
      "postcss-import",
      "postcss-cssnext"
    ]
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/src/style-mock.js"
    }
  }
