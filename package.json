    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-flowtype": "2.29.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jasmine": "^2.1.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
