    "fs-extra": "1.0.0",
    "fuzzysearch": "^1.0.3",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.26.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "json-loader": "0.5.4",
    "normalize.css": "^5.0.0",
    "object-assign": "4.1.0",
    "postcss-cssnext": "^2.7.0",
