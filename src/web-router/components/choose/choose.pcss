:root {
  --contracted: #dfe3e2;
  --color: #000;
}

.static .button {
  background-color: var(--contracted);
}

.checkbox {
  display: inline-flex;
}

.checkbox .gel-icon {
  margin: auto;
  margin-left: 6px;
  width: 16px;
  height: auto;
  padding: 2px;
  border: solid 2px;

  &:hover {
    cursor: pointer;
  }
}

.routables-left .break {
  display: none;
}

.all-visible {
  padding-right: 14px;
  line-height: 38px;
  height: 38px;
  text-align: right;
}
